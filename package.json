  "scripts": {
    "clean": "yarn clean:dist; rm -rf ./releases",
    "clean:dist": "rm -rf ./dist; rm -rf .parcel-cache",
    "postinstall": "NODE_ENV=staging; node scripts/create-env.mjs && node ./scripts/create_manifest.js",
    "prettier": "prettier --write --log-level warn \"src/**/*.{js,jsx,json,ts,tsx,css,scss,md}\"",
    "predev": "yarn clean && yarn prettier",
    "dev": "NODE_ENV=staging; yarn clean && yarn postinstall && parcel watch src/manifest.json --host localhost --no-source-maps",
    "dev:production": "NODE_ENV=production; yarn clean && parcel build src/manifest.json --no-cache --no-source-maps --no-scope-hoist",
    "dist": "NODE_ENV=production; yarn build:all; NODE_ENV=staging; yarn build:all",
    "dist:all": "yarn clean; yarn dist:udme; yarn dist:upio",
    "dist:udme": "THEME=udme; yarn dist",
    "dist:upio": "THEME=upio; yarn dist",
    "build:all": "yarn clean:dist; yarn build:chrome:archives",
    "build:extension": "parcel build src/manifest.json --no-cache --no-source-maps --no-scope-hoist",
    "build:chrome": "yarn build:chrome:manifest && yarn build:extension",
    "build:chrome:manifest": "node scripts/create-env.mjs && node ./scripts/create_manifest.js chrome",
    "build:chrome:archives": "yarn build:chrome && yarn build:chrome:zip",
    "build:chrome:zip": "node scripts/create-zip.mjs chrome",
    "build:firefox": "yarn build:firefox:manifest && yarn build:extension",
    "build:firefox:manifest": "node scripts/create-env.mjs && node ./scripts/create_manifest.js firefox",
    "build:firefox:archives": "yarn build:firefox && yarn build:firefox:zip",
    "build:firefox:zip": "node scripts/create-zip.mjs firefox",
    "lint": "eslint --fix .",
    "test": "NODE_ENV=staging; DOTENV_CONFIG_PATH=./.env.staging jest --runInBand",
    "test:ci": "yarn test --coverage --ci --no-colors --forceExit --cacheDirectory .jest-cache",
    "typecheck": "tsc --project tsconfig.json --noEmit"
  },
