{
  "name": "ud-extension",
  "version": "3.1.0",
  "license": "MIT",
  "author": {
    "email": "eng@unstoppabledomains.com",
    "name": "Unstoppable Domains"
  },
  "homepage": "https://unstoppabledomains.com/extension",
  "scripts": {
    "prettier": "prettier --write --log-level warn \"src/**/*.{js,jsx,json,ts,tsx,css,scss,md}\"",
    "predev": "rimraf .parcel-cache dist && yarn prettier",
    "dev": "parcel watch src/manifest.json --host localhost",
    "build": "parcel build src/manifest.json --no-cache",
    "postbuild": "node scripts/remove-source-maps.mjs && node scripts/create-zip.mjs",
    "create-env": "node scripts/create-env.mjs",
    "create-manifest": "node create_manifest.js",
    "postinstall": "npm run create-env && npm run create-manifest"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fireblocks/ncw-js-infra": "1.0.20-experimental.1",
    "@fireblocks/ncw-js-sdk": "12.1.3-experimental.1",
    "@mui/icons-material": "^5.15.20",
    "@mui/material": "^5.15.20",
    "@mui/styles": "^5.15.21",
    "@tanstack/react-query": "^5.45.1",
    "@tanstack/react-query-devtools": "^5.45.1",
    "launchdarkly-react-client-sdk": "^3.3.2",
    "qs": "^6.12.3",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-qrcode-logo": "^3.0.0",
    "react-router-dom": "^6.23.1",
    "use-async-effect": "^2.2.7",
    "uns": "https://github.com/unstoppabledomains/uns#v0.8.35"
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@parcel/config-webextension": "^2.12.0",
    "@parcel/transformer-svg-react": "^2.12.0",
    "@types/chrome": "^0.0.268",
    "@types/node": "^20.14.6",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "assert": "^2.1.0",
    "buffer": "^5.5.0||^6.0.0",
    "dotenv": "^16.4.5",
    "filenamify": "^6.0.0",
    "fs-extra": "^11.2.0",
    "glob": "^10.4.2",
    "nodemon": "^3.1.4",
    "parcel": "^2.12.0",
    "parcel-plugin-static-files-copy": "^2.6.0",
    "parcel-reporter-static-files-copy": "^1.5.3",
    "prettier": "^3.3.2",
    "process": "^0.11.10",
    "querystring-es3": "^0.2.1",
    "rimraf": "^5.0.7",
    "typescript": "^5.4.5",
    "zip-lib": "^1.0.4"
  },
  "staticFiles": {
    "staticPath": "static",
    "watcherGlob": "**"
  }
}
